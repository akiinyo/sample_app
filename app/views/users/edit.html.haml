- provide(:title, 'Edit user')
%h1 Update your profile

= form_for(@user) do |f|
  = render 'shared/error_messages', object: f.object
  = render 'fields', f: f
  = f.submit "Save changes", class: "btn btn-large btn-primary"
= gravatar_for @user
%a{href: 'http://gravatar.com/emails', target: "_blank" }Change
